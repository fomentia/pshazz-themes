{
    "plugins": [ "git", "z", "aliases" ],
    "prompt": [
	[ "darkgreen", "", "$user" ],
	[ "darkyellow", "", " $dir" ],
	[ "", "", " $git_lbracket$git_branch" ],
	[ "red", "", "$git_dirty" ],
	[ "", "", "$git_rbracket"],
	[ "white", "", " >" ]
    ],
    "git": {
	"prompt_dirty": "*",
	"prompt_lbracket": "[",
	"prompt_rbracket": "]"
    }
}
